<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.torange.api.dbmanager.dao.DbManagerDAO">

    <select id="selectDatabaseConfig" parameterType="loginUserInfoVO" resultType="dbManagerVO">
        <![CDATA[
        SELECT SEQ,
               DB_TYPE,
               DB_DRIVER_CL_NM,
               DB_JDBC_PATH || DB_JDBC_FILE AS JDBC_PATH
        FROM DB_CONFIG
        ]]>
    </select>
</mapper>